<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

<servlet>
    <servlet-name>NoteBook</servlet-name>
    <servlet-class>model.NotebookServlet</servlet-class>
</servlet>
<servlet>
    <servlet-name>Search</servlet-name>
    <servlet-class>model.NotebookSearchServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>NoteBook</servlet-name>
    <url-pattern>/NoteBook/*</url-pattern>
</servlet-mapping>

<servlet-mapping>
    <servlet-name>Search</servlet-name>
    <url-pattern>/NoteBook/searchResult</url-pattern>
</servlet-mapping>



</web-app>